[
    {
        "name": "sample-webapp",
        "image": "863529286148.dkr.ecr.ap-northeast-1.amazonaws.com/sample-image:latest",
        "cpu": 333,
        "memoryReservation": 600,
        "essential": true,
        "portMapptings": [
